{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "globals": {
    "describe": "writable",
    "it": "writable",
    "expect": "writable"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "no-unused-vars": "off"
  }
}
